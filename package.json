{
  "name": "@contentmunch/muncher-ui",
  "version": "0.5.4",
  "description": "Muncher UI components",
  "author": "asikpradhan",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/contentmunch/muncher-ui.git"
  },
  "scripts": {
    "start": "npm run storybook",
    "prebuild": "rimraf lib",
    "build": "npm run build:types && npm run build:js ",
    "postbuild": "rsync -avz --exclude={\"*.ts\",\"*.tsx\",\"__snapshots__\",\"*.js\",\"*.mdx\"} src/ lib/",
    "build:types": "cp tsconfig-copy.json tsconfig.json && tsc --emitDeclarationOnly",
    "postbuild:types": "rimraf lib/**/*.test.d.ts lib/**/*.stories.d.ts lib/setupTests.d.ts",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "prepublishOnly": "npm run build",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-essentials": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addon-storyshots": "^6.0.26",
    "@storybook/node-logger": "^6.0.26",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.26",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.19.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "babel-loader": "^8.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-is": "^17.0.0",
    "react-scripts": "3.4.4",
    "react-test-renderer": "^17.0.0",
    "rimraf": "^3.0.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1"
  }
}
