{
  "name": "@contentmunch/muncher-ui",
  "version": "4.0.2",
  "description": "Muncher UI components",
  "author": "asikpradhan",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o docs",
    "deploy": "npm run build-storybook",
    "move-files": "cp public/CNAME docs/CNAME",
    "rename-files": "cd docs/assets; for file in _*.js*; do mv ${file} `echo $file | cut -c 2-`; done; sed -i -e 's/_commonjsHelpers/commonjsHelpers/g' *.js*",
    "clean-files": "rm -rf docs/assets/*.js*-e",
    "postdeploy": "npm run move-files && npm run rename-files && npm run clean-files"
  },
  "main": "dist/muncher-ui.umd.js",
  "module": "dist/muncher-ui.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentmunch/muncher-ui.git"
  },
  "directories": {
    "lib": "dist"
  },
  "bugs": {
    "url": "https://github.com/contentmunch/muncher-ui/issues"
  },
  "homepage": "https://ui.muncher.dev/",
  "keywords": [
    "html5",
    "reactjs",
    "ui",
    "component driven"
  ],
  "peerDependencies": {
    "react": ">=18.x",
    "react-dom": ">=18.x"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^7.5.0",
    "@storybook/addon-essentials": "^7.5.0",
    "@storybook/addon-interactions": "^7.5.0",
    "@storybook/addon-links": "^7.5.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.0",
    "@storybook/react": "^7.5.0",
    "@storybook/react-vite": "^7.5.0",
    "@storybook/testing-library": "^0.2.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-storybook": "^0.6.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.3",
    "storybook": "^7.5.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-css-injected-by-js": "^3.3.0",
    "vite-plugin-dts": "^3.6.0",
    "vite-plugin-linter": "^2.0.7",
    "vite-tsconfig-paths": "^4.2.1"
  },
  "dependencies": {
    "moment": "^2.29.4"
  }
}
